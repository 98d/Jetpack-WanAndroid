<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_navi">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.win.ft_home.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_tree"
        android:name="com.win.ft_home.ui.tree.TreeFragment"
        android:label="@string/title_tree"
        tools:layout="@layout/fragment_tree" />

    <fragment
        android:id="@+id/navigation_project"
        android:name="com.win.ft_home.ui.navi.NavigationFragment"
        android:label="@string/title_project"
        tools:layout="@layout/fragment_navigation" />

    <fragment
        android:id="@+id/navigation_navi"
        android:name="com.win.ft_home.ui.project.ProjectFragment"
        android:label="@string/title_navi"
        tools:layout="@layout/fragment_project" />

</navigation>